<!doctype html>
<html>
<head>

  <title>Welcome to Hike Maps</title>

  <% include ./partials/_generalhead %>

  <script type="text/javascript" src="/public/scripts/my_maps.js"></script>
  <script type="text/javascript" src="/public/scripts/pub_utils.js"></script>

</head>
<body>

<% include ./partials/_header %>

<main class="page-content">

     <div class="container">
    <!-- Portfolio Item Heading -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                <!-- Main title -->
                <small></small>
            </h1>
        </div>
    </div>
    <!-- /.row -->



    <!-- Portfolio Item Row -->
    <div class="row">

        <div class="map view col-md-6">
            <div class="mapview" >
              <div id="map"  style="height: 40rem"></div>
            </div>
        </div>

        <div class="col-md-6">


          <div class="editscreen" style="border: solid 1px">
            <div class="userhikelist text-center">
              <h4 class="text-left">YOUR INPUTTED HIKES</h4>
              <ul class="myhikes text-left" style="list-style-type: none;">
              <button type="button" class="createnew btn btn-info text-left">CREATE NEW MAP</button>
              </ul>
            </div>
          </div>


        </div>

    </div>

  </div>

</main>


</body>
</html>





